<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
     xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/adsdk_root_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

     <RelativeLayout
         android:id="@+id/com_iinmobi_adsdk_head"
         android:layout_width="fill_parent"
         android:layout_height="43.33dp"
         android:background="@drawable/com_iinmobi_adsdk_new_topbar_bg" >

         <ImageView
             android:id="@+id/com_iinmobi_adsdk_pull"
             android:layout_width="wrap_content"
             android:layout_height="match_parent"
             android:src="@drawable/com_iinmobi_adsdk_new_pull" />

         <ImageView
             android:id="@+id/com_iinmobi_adsdk_imageView2"
             android:layout_width="wrap_content"
             android:layout_toRightOf="@+id/com_iinmobi_adsdk_pull"
             android:layout_height="match_parent"
             android:layout_marginLeft="16dp"
             android:src="@drawable/com_iinmobi_adsdk_new_topbar_icon" />

         <TextView
             android:id="@+id/com_iinmobi_adsdk_textView"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_centerVertical="true"
             android:layout_marginLeft="6.6dp"
             android:layout_toRightOf="@+id/com_iinmobi_adsdk_imageView2"
             android:textColor="#ffffff"
             android:textSize="16sp"
             android:text="TOP FREE GAMES" />
     </RelativeLayout>

	<ListView
        android:id="@+id/com_iinmobi_adsdk_listView1"
        android:background="#e3e3e3"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:cacheColorHint="#00000000"
        android:fadingEdge="none"
        android:fastScrollEnabled="false"
        android:fadeScrollbars="true"
        android:drawSelectorOnTop="false"
        android:layout_below="@+id/com_iinmobi_adsdk_head"
        android:headerDividersEnabled="false"
        android:divider="@null"/>
</RelativeLayout>